<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <script src="https://jandalhuysen.github.io/react.production.min.js"></script>
    <script src="https://jandalhuysen.github.io/react-dom.production.min.js"></script>
    <script src="https://jandalhuysen.github.io/babel.min.js"></script>
    <script src="https://jandalhuysen.github.io/tailwindcss.js"></script>
    <script src="https://jandalhuysen.github.io/material-ui.production.min.js"></script>
    <title>Test</title>
  </head>
  <body>
    <div id="app"></div>
    <script type="text/babel">
      function App() {
        return (
          <div>
            <form action="/form" method="post">
              <input type="text" name="name" />
              <br />
              <input type="text" name="surname" />
              <br />
              <button type="submit">Submit</button>
            </form>
          </div>
        );
      }
      ReactDOM.render(<App />, document.getElementById("app"));
    </script>
  </body>
</html>
